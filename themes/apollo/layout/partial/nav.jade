ul.nav.nav-list
    each value, key in theme.menu
        li.nav-list-item
            - var re = /^(http|https):\/\/*/gi;
            - var tar = re.test(value) ? "_blank" : "_self"
            - var act = !re.test(value) && ("/"+page.current_url === value || (page.current_url === undefined && value === "/motivational/"))

            - console.log(page.current_url)
            - console.log(value)
            a.nav-list-link(class={active: act} href=url_for(value) target=tar)
                != key.toUpperCase()
